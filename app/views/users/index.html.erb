<%= render 'books/books', {book: @book, user: @user} %>


  <div class="col-lg-8">
  <table class="table table-striped">
   <p id="notice"></p>
    <div class='col-md-8 offset-md-1'>
      <h2>Users</h2>
      <table class='table table-hover table-inverse'>
     <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th colspan="1"></th>
    </tr>
    </thead>
   </div>
     <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= image_tag user.get_profile_image(100, 100) %></td>
        <td><%= user.name %></td>
        <td><%= link_to "Show", user_path(user.id) %></td>
      </tr>
    <% end %>
    </tbody>
  </div>
</div>
</div>
