<h1>User info</h1>
  <%= form_with model: @book do |f| %>
    <%= f.file_field :image, accept: "image/*" %>
    <h4>name</h4>
      <%= f.text_field :user_name %>
    <h4>introduction</h4>
      <%= f.text_area :caption %>
  <% end %>




<h1>New book</h1>
  <%= form_with model: @book do |f| %>
    <h4>Title</h4>
      <%= f.file_field :title %>
    <h4>Opinion</h4>
      <%= f.text_area :caption %>
    <%= f.submit "Create book" %>
  <% end %>